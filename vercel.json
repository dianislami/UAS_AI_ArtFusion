{
  "version": 2,
  "builds": [
    {
      "src": "ArtFusion/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "ArtFusion_Backend/api/**/*.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "memory": 3008,
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/ArtFusion_Backend/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/ArtFusion/$1"
    }
  ],
  "outputDirectory": "ArtFusion/dist"
}